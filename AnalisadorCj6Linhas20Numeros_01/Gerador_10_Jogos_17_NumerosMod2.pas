              IIf(Stgr_TodosNumerosP17.Cells[viContar - 0, 0].ToInteger > 25, (Stgr_TodosNumerosP17.Cells[viContar - 0, 0].ToInteger - 25).ToString,
              Stgr_TodosNumerosP17.Cells[viContar - 0, 0]);
