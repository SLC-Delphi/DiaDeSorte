         Stgr_Proc2Combinacoes08NaoSorteados.Cells[04,viLinhas] := Stgr_Proc2Combinacoes01Sconv_1NS.Cells[0,viContar03];
       // --> ordena os dados
         Falso_Linha2TodosNumeros;
         for viContarColunas := 01 to 2 do
