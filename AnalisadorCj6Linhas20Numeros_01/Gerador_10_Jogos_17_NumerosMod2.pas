               Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_Proc2Combinacoes01Sconv_1NS.Cells[viContarColunas+1, viContar03]))*-1, 2] := 'V';
         end;
         for viContarColunas := 01 to 06 do
         begin
            Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_Proc2Combinacoes06NaoSorteados.Cells[viContarColunas+2, viContar04])), 2] := 'V';
         end;
         ViContarCelulas := 1;
         FOR viContarColunas :=1 TO 25 DO
         BEGIN
            IF (Stgr_Todos_Numeros.Cells[viContarColunas,2]='V') THEN
            BEGIN
               Stgr_Proc2Combinacoes08NaoSorteados.Cells [ViContarCelulas+4, viLinhas] := RIGHTSTR ('0'+(Stgr_Todos_Numeros.Cells[viContarColunas,0]),2) ;
