   viContar08, viContar04, viContarReprovado1, viLinhas, viContarColunas, ViContarCelulas: integer;
   vbReprovado : Boolean;
Begin
   Pgbr_GERARCOMB0.Position := 0;
   viLinhas := 1;
   for viContar08 := 1 to Trunc((Stgr_08NumerosSorteados.RowCount-1)/vcrDivisor2) do
   begin
      if Stgr_08NumerosSorteados.Cells[2,viContar08] = 'F' then
         Continue;
      for viContar04 := 1 to Stgr_Combinacoes04Modo2.RowCount-1 do
      begin
         if Stgr_Combinacoes04Modo2.Cells[2,viContar04] = 'F' then
            Continue;
         // --> Processamento sempre da mesma linha Original
         if Not(Stgr_08NumerosSorteados.Cells[1,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[1,viContar04].ToInteger)  then
            Continue;

         vbReprovado := False;
         for viContarReprovado1 := 8 to 11 do
         begin
            // Inicial não aparece na outra
            if (Stgr_08NumerosSorteados.Cells[03,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[04,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[05,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[06,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[07,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[08,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[09,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_08NumerosSorteados.Cells[10,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_03NumerosNaoSorteados.Cells[03,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_03NumerosNaoSorteados.Cells[04,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
            if (Stgr_03NumerosNaoSorteados.Cells[05,viContar08].Tointeger = Stgr_Combinacoes04Modo2.Cells[viContarReprovado1,viContar04].ToInteger)  then
               vbReprovado := True;
         end; // For
         if vbReprovado then
            Continue;
         Stgr_09NumerosSorteados.Cells[00,viLinhas] := viLinhas.ToString;
         Stgr_09NumerosSorteados.Cells[01,viLinhas] := Stgr_08NumerosSorteados.Cells[1,viContar08];
         Stgr_09NumerosSorteados.Cells[02,viLinhas] := viContar08.ToString+'.'+viContar04.ToString;
          Stgr_06NumerosNaoSorteados.Cells[00,viLinhas] := viLinhas.ToString;
          Stgr_06NumerosNaoSorteados.Cells[01,viLinhas] := Stgr_08NumerosSorteados.Cells[1,viContar08];
          Stgr_06NumerosNaoSorteados.Cells[02,viLinhas] := viContar08.ToString+'.'+viContar04.ToString;
         Stgr_09NumerosSorteados.Cells[03,viLinhas] := Stgr_08NumerosSorteados.Cells[03,viContar08];
         Stgr_09NumerosSorteados.Cells[04,viLinhas] := Stgr_08NumerosSorteados.Cells[04,viContar08];
         Stgr_09NumerosSorteados.Cells[05,viLinhas] := Stgr_08NumerosSorteados.Cells[05,viContar08];
         Stgr_09NumerosSorteados.Cells[06,viLinhas] := Stgr_08NumerosSorteados.Cells[06,viContar08];
         Stgr_09NumerosSorteados.Cells[07,viLinhas] := Stgr_08NumerosSorteados.Cells[07,viContar08];
         Stgr_09NumerosSorteados.Cells[08,viLinhas] := Stgr_08NumerosSorteados.Cells[08,viContar08];
         Stgr_09NumerosSorteados.Cells[09,viLinhas] := Stgr_08NumerosSorteados.Cells[09,viContar08];
         Stgr_09NumerosSorteados.Cells[10,viLinhas] := Stgr_08NumerosSorteados.Cells[10,viContar08];
          Stgr_06NumerosNaoSorteados.Cells[03,viLinhas] := Stgr_03NumerosNaoSorteados.Cells[3,viContar08];
          Stgr_06NumerosNaoSorteados.Cells[04,viLinhas] := Stgr_03NumerosNaoSorteados.Cells[4,viContar08];
          Stgr_06NumerosNaoSorteados.Cells[05,viLinhas] := Stgr_03NumerosNaoSorteados.Cells[5,viContar08];
         if (Stgr_Combinacoes04Modo2.Cells[08,viContar04].Tointeger >= 1) then
         begin
            Stgr_09NumerosSorteados.Cells[11,viLinhas] := Stgr_Combinacoes04Modo2.Cells[08,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[06,viLinhas] := Stgr_Combinacoes04Modo2.Cells[09,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[07,viLinhas] := Stgr_Combinacoes04Modo2.Cells[10,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[08,viLinhas] := Stgr_Combinacoes04Modo2.Cells[11,viContar04];
         end
         else
         if (Stgr_Combinacoes04Modo2.Cells[09,viContar04].Tointeger >= 1) then
         begin
            Stgr_09NumerosSorteados.Cells[11,viLinhas] := Stgr_Combinacoes04Modo2.Cells[09,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[06,viLinhas] := Stgr_Combinacoes04Modo2.Cells[08,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[07,viLinhas] := Stgr_Combinacoes04Modo2.Cells[10,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[08,viLinhas] := Stgr_Combinacoes04Modo2.Cells[11,viContar04];
         end
         else
         if (Stgr_Combinacoes04Modo2.Cells[10,viContar04].Tointeger >= 1) then
         begin
            Stgr_09NumerosSorteados.Cells[11,viLinhas] := Stgr_Combinacoes04Modo2.Cells[10,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[06,viLinhas] := Stgr_Combinacoes04Modo2.Cells[08,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[07,viLinhas] := Stgr_Combinacoes04Modo2.Cells[09,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[08,viLinhas] := Stgr_Combinacoes04Modo2.Cells[11,viContar04];
         end
         else
         if (Stgr_Combinacoes04Modo2.Cells[11,viContar04].Tointeger >= 1) then
         begin
            Stgr_09NumerosSorteados.Cells[11,viLinhas] := Stgr_Combinacoes04Modo2.Cells[11,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[06,viLinhas] := Stgr_Combinacoes04Modo2.Cells[08,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[07,viLinhas] := Stgr_Combinacoes04Modo2.Cells[09,viContar04];
            Stgr_06NumerosNaoSorteados.Cells[08,viLinhas] := Stgr_Combinacoes04Modo2.Cells[10,viContar04];
         end;
         vbReprovado := False;
         for viContarReprovado1 := 3 to 11 do
         begin
            if (Stgr_09NumerosSorteados.Cells[viContarReprovado1,viLinhas].ToInteger = (Stgr_06NumerosNaoSorteados.Cells[03,viLinhas].ToInteger * -1) ) then
               vbReprovado := True;
            if (Stgr_09NumerosSorteados.Cells[viContarReprovado1,viLinhas].ToInteger = (Stgr_06NumerosNaoSorteados.Cells[04,viLinhas].ToInteger * -1) ) then
               vbReprovado := True;
            if (Stgr_09NumerosSorteados.Cells[viContarReprovado1,viLinhas].ToInteger = (Stgr_06NumerosNaoSorteados.Cells[05,viLinhas].ToInteger * -1) ) then
               vbReprovado := True;
            if (Stgr_09NumerosSorteados.Cells[viContarReprovado1,viLinhas].ToInteger = (Stgr_06NumerosNaoSorteados.Cells[06,viLinhas].ToInteger * -1) ) then
               vbReprovado := True;
            if (Stgr_09NumerosSorteados.Cells[viContarReprovado1,viLinhas].ToInteger = (Stgr_06NumerosNaoSorteados.Cells[07,viLinhas].ToInteger * -1) ) then
               vbReprovado := True;
            if (Stgr_09NumerosSorteados.Cells[viContarReprovado1,viLinhas].ToInteger = (Stgr_06NumerosNaoSorteados.Cells[08,viLinhas].ToInteger * -1) ) then
               vbReprovado := True;
         end;
         // --> ordena os dados
         Falso_Linha1TodosNumeros;
         for viContarColunas := 01 to 09 do
         begin
            Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_09NumerosSorteados.Cells[viContarColunas+2, viLinhas])), 1] := 'V';
         end;
          ViContarCelulas := 1;
          FOR viContarColunas :=1 TO 25 DO
          BEGIN
               IF (Stgr_Todos_Numeros.Cells[viContarColunas,1]='V') THEN
               BEGIN
                    Stgr_09NumerosSorteados.Cells [ViContarCelulas+2, viLinhas] := RIGHTSTR ('0'+(Stgr_Todos_Numeros.Cells[viContarColunas,0]),2) ;
                    ViContarCelulas := ViContarCelulas+1;
               END; // IF (STRGR_TODOS_NUMEROS
          END; // FOR viContarColunas
         Falso_Linha1TodosNumeros;
         for viContarColunas := 01 to 06 do
         begin
            Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_06NumerosNaoSorteados.Cells[viContarColunas+2, viLinhas]))*-1, 1] := 'V';
         end;
          ViContarCelulas := 1;
          FOR viContarColunas :=1 TO 25 DO
          BEGIN
               IF (Stgr_Todos_Numeros.Cells[viContarColunas,1]='V') THEN
               BEGIN
                    Stgr_06NumerosNaoSorteados.Cells [ViContarCelulas+2, viLinhas] := '-'+(Stgr_Todos_Numeros.Cells[viContarColunas,0]) ;
                    ViContarCelulas := ViContarCelulas+1;
               END; // IF (STRGR_TODOS_NUMEROS
          END; // FOR viContarColunas
          // --> Contador de linha
         if Not(vbReprovado) then
            viLinhas := viLinhas + 1;
      end;
      if ((viContar08 mod 180) = 0) then
      begin
         Stgr_09NumerosSorteados.RowCount := viLinhas;
         Stgr_09NumerosSorteados.Repaint;
         Stgr_06NumerosNaoSorteados.RowCount := viLinhas;
         Stgr_06NumerosNaoSorteados.Repaint;
         Pgbr_GERARCOMB0.Position := Trunc((viContar08/(Stgr_08NumerosSorteados.RowCount-1))*100);
         Pgbr_GERARCOMB0.Repaint;
         PANEL_TITULO_SIMULACAO.Repaint;
      end;
      if ((viContar08 mod 1800) = 0) then
      begin
         Stgr_09NumerosSorteados.RowCount := viLinhas;
         Stgr_09NumerosSorteados.Repaint;
         Stgr_06NumerosNaoSorteados.RowCount := viLinhas;
         Stgr_06NumerosNaoSorteados.Repaint;
         GravarComb08e04;
         Grid09NumerosSorteados;
         Grid06NumerosNaoSorteados;
         viLinhas := 1;
         Pgbr_GERARCOMB0.Position := Trunc((viContar08/(Stgr_08NumerosSorteados.RowCount-1))*100);
         Pgbr_GERARCOMB0.Repaint;
         PANEL_TITULO_SIMULACAO.Repaint;
         Sleep (75);
         PANEL_TITULO_SIMULACAO.Repaint;
      end;
   end;
   Pgbr_GERARCOMB0.Position := Trunc((viContar08/(Stgr_08NumerosSorteados.RowCount-1))*100);
   Pgbr_GERARCOMB0.Repaint;
   Stgr_09NumerosSorteados.RowCount := viLinhas;
   Stgr_06NumerosNaoSorteados.RowCount := viLinhas;
   GravarComb08e04;
   Pgbr_GERARCOMB0.Position := 100;
   Pgbr_GERARCOMB0.Repaint;
   PANEL_TITULO_SIMULACAO.Repaint;
   Stgr_09NumerosSorteados.RowCount := viLinhas;
   Stgr_09NumerosSorteados.Repaint;
   Stgr_06NumerosNaoSorteados.RowCount := viLinhas;
   Stgr_06NumerosNaoSorteados.Repaint;
   Sleep(900);
   CarregarComb08e04;
   PANEL_TITULO_SIMULACAO.Repaint;
End;


Procedure T_F_GERADOR_25d17x8M2.GravarComb08e04();
Var
   viContarLinhas, viContarColunas, ViContarCelulas: integer;
   vsDADOS08P4_09NUMEROS, vsDADOS08P4_06NUMEROS, vcQuery, vcNomeTabela : String;
Begin
   vcNomeTabela := ' CJTO_17POR8_12_BLOCO4P2';
   Pgbr_GERARCOMB1.Position := 0;
   Pgbr_GERARCOMB1.Repaint;
   for viContarLinhas := 1 to Stgr_09NumerosSorteados.RowCount-1 do
   begin
      vsDADOS08P4_09NUMEROS := '';
      vsDADOS08P4_06NUMEROS := '';
      // --> preparando os dados para gravar
      for viContarColunas := 01 to 09 do
      begin
         vsDADOS08P4_09NUMEROS := vsDADOS08P4_09NUMEROS + RightStr('0'+Stgr_09NumerosSorteados.Cells[viContarColunas+2,viContarlinhas],2)+' ' ;
      end;
      for viContarColunas := 01 to 06 do
      begin
         vsDADOS08P4_06NUMEROS := vsDADOS08P4_06NUMEROS + RightStr('0'+((Stgr_06NumerosNaoSorteados.Cells[viContarColunas+2,viContarlinhas].ToInteger)*-1).ToString,2)+' ' ;
      end;
      vcQuery  := 'INSERT INTO ' +vcNomeTabela+ ' (BLOCO, DADOS08P4_09NUMEROS, DADOS08P4_06NUMEROS) VALUES('
              +#39+  TRIM(Stgr_09NumerosSorteados.Cells[1,viContarlinhas])  + #39 +','
              +#39+  TRIM(vsDADOS08P4_09NUMEROS)  + #39 +','
              +#39+  TRIM(vsDADOS08P4_06NUMEROS)
          +#39+ ');';
      DM1.IBQ_Combinacoes.SQL.Clear;
      DM1.IBQ_Combinacoes.SQL.Add(vcQuery)  ;
      DM1.IBQ_Combinacoes.ExecSQL;
      if (viContarLinhas MOD 180) = 0 then
      BEGIN
         Pgbr_GERARCOMB1.Position := Trunc((viContarLinhas/(Stgr_09NumerosSorteados.RowCount-1))*100);
         Pgbr_GERARCOMB1.Repaint;
         Sleep (20);
      END;
    end;
   DM1.IBQ_Combinacoes.SQL.Clear;
   DM1.IBQ_Combinacoes.SQL.Add('COMMIT')  ;
   DM1.IBQ_Combinacoes.ExecSQL;
   DM1.IBQ_Combinacoes.Transaction.StartTransaction;
   DM1.IBQ_Combinacoes.Transaction.Commit;
   DM1.IBQ_Combinacoes.Transaction.StartTransaction;
   Pgbr_GERARCOMB1.Position := 100;
   Pgbr_GERARCOMB1.Repaint;
End;


Procedure T_F_GERADOR_25d17x8M2.CarregarComb08e04();
