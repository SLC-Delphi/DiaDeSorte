Begin
   Grid09NumerosSorteados;
   Grid06NumerosNaoSorteados;
   sleep(2000);
   Stgr_09NumerosSorteados.RowCount := 50;
   Stgr_06NumerosNaoSorteados.RowCount := 50;
   vcQuery :='Select bloco || ' +#39+'-'+#39+ ' || dados08p4_09numeros || ' +#39+'-'+#39+ ' ||dados08p4_06numeros as DADOS ';
   vcQuery := vcQuery + 'from CJTO_17POR8_12_BLOCO4P2 ';
   vcQuery := vcQuery + 'group by DADOS ';
   DM1.IBQ_Combinacoes.SQL.Clear;
