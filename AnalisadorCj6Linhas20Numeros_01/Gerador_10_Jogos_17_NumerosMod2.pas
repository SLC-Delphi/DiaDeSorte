   Scbx_Processamento2.HorzScrollBar.Position := 8252;
   AtualizarTelaScroll;
   Pgbr_GerarCombinacoes2.Position :=0;
   Pgbr_GerarCombinacoes2.Repaint;
   viLinhas := 1;
   for viContar04 := 1 to Trunc((Stgr_Proc2Combinacoes14Sorteados.RowCount-1)/vcrDivisor2) do
