Procedure T_F_GERADOR_25d17x8M2.GerarComb02B2Modelo1(viModo: SmallInt);
Var
   viContarCombinacoes, viQTDCelulasComb, viContar, viLinhas: integer;
Begin
   viLinhas := 1;
   For viContarCombinacoes := 1 To Trunc((Stgr_Base17.RowCount - 1) / 1) Do
   Begin
      If Stgr_Base17.Cells[1, viContarCombinacoes] <> 'V' Then
         Continue;
      Falso_Linha1TodosNumerosP17();
      Falso_Linha2TodosNumerosP17();
      FOR viContar := 2 TO 18 DO
      Begin
         Stgr_TodosNumerosP17.Cells[(STRtoINT(Stgr_Base17.Cells[viContar, viContarCombinacoes])), 1] := 'V';
         If viContar <= 2 Then
            Stgr_TodosNumerosP17.Cells[25 + (STRtoINT(Stgr_Base17.Cells[viContar, viContarCombinacoes])),
              1] := 'V';
      End;
      FOR viContar := 2 TO 9 DO
      Begin
         Stgr_TodosNumerosP17.Cells[(STRtoINT(Stgr_Base08.Cells[viContar, viContarCombinacoes])), 2] := 'V';
         If viContar <= 2 Then
            Stgr_TodosNumerosP17.Cells[25 + (STRtoINT(Stgr_Base08.Cells[viContar, viContarCombinacoes])),
              2] := 'V';
      End;
      Stgr_TodosNumerosP17.Repaint;
      viContar := 2;
      While viContar <= 26 Do
      Begin
         If ((Stgr_TodosNumerosP17.Cells[viContar - 1, 1] = 'V') And
           (Stgr_TodosNumerosP17.Cells[viContar -0, 2] = 'V')) Then
         Begin
            Stgr_Combinacoes02Modo2B2.Cells[0, viLinhas] := viLinhas.ToString;
            Stgr_Combinacoes02Modo2B2.Cells[1, viLinhas] := Stgr_Base17.Cells[0, viContarCombinacoes];
            Stgr_Combinacoes02Modo2B2.Cells[2, viLinhas] := 'V';
            Stgr_Combinacoes02Modo2B2.Cells[3, viLinhas] :=
              IIf(Stgr_TodosNumerosP17.Cells[viContar - 1, 0].ToInteger > 25, (Stgr_TodosNumerosP17.Cells[viContar - 1, 0].ToInteger - 25).ToString,
              Stgr_TodosNumerosP17.Cells[viContar - 1, 0]);
            Stgr_Combinacoes02Modo2B2.Cells[4, viLinhas] :=
              IIf(Stgr_TodosNumerosP17.Cells[viContar - 0, 0].ToInteger > 25, (Stgr_TodosNumerosP17.Cells[viContar - 0, 0].ToInteger - 25).ToString,
              Stgr_TodosNumerosP17.Cells[viContar - 0, 0]);
            //--> acertando o positivo e negativo, alternando nas linhas
            if viModo = 2 then
            begin
               Stgr_Combinacoes02Modo2B2.Cells[6, viLinhas] := (Stgr_Combinacoes02Modo2B2.Cells[3, viLinhas].ToInteger * -1).ToString;
               Stgr_Combinacoes02Modo2B2.Cells[7, viLinhas] := Stgr_Combinacoes02Modo2B2.Cells[4, viLinhas];
            end
            else if viModo = 3 then
            begin
               Stgr_Combinacoes02Modo2B2.Cells[6, viLinhas] := Stgr_Combinacoes02Modo2B2.Cells[3, viLinhas];
