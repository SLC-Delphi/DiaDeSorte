            Stgr_Combinacoes02Modo2B1.Cells[6, viLinhas] := Stgr_Combinacoes02Modo2B1.Cells[3, viLinhas];
            Stgr_Combinacoes02Modo2B1.Cells[7, viLinhas] := Stgr_Combinacoes02Modo2B1.Cells[4, viLinhas];
            viLinhas := viLinhas + 1;
         End;
