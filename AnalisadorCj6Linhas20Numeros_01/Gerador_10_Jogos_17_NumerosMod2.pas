Procedure T_F_GERADOR_25d17x8M2.GravarComb09e02M1();
Var
   viContarLinhas, viContarColunas, ViContarCelulas: integer;
   vsDADOS11NUMEROS, vsDADOS06NUMEROS, vcQuery, vcNomeTabela : String;
Begin
   vcNomeTabela := ' CJTO_17POR8_12_BLOCO2P2M1';
   Pgbr_GERARCOMB1.Position := 0;
   Pgbr_GERARCOMB1.Repaint;
   for viContarLinhas := 1 to Stgr_11NumerosSorteadosM1.RowCount-1 do
   begin
      vsDADOS11NUMEROS := '';
      vsDADOS06NUMEROS := '';
      // --> preparando os dados para gravar
      for viContarColunas := 01 to 11 do
      begin
         vsDADOS11NUMEROS := vsDADOS11NUMEROS + RightStr('0'+Stgr_11NumerosSorteadosM1.Cells[viContarColunas+2,viContarlinhas],2)+' ' ;
      end;
      for viContarColunas := 01 to 06 do
      begin
         vsDADOS06NUMEROS := vsDADOS06NUMEROS + RightStr('0'+(Stgr_06NumerosNaoSorteadosM1.Cells[viContarColunas+2,viContarlinhas].ToInteger *-1).ToString,2)+' ' ;
      end;
      vcQuery  := 'INSERT INTO ' +vcNomeTabela+ ' (BLOCO, dados09_11numeros, DADOS09_06NUMEROS) VALUES('
              +#39+  TRIM(Stgr_11NumerosSorteadosM1.Cells[1,viContarlinhas])  + #39 +','
              +#39+  TRIM(vsDADOS11NUMEROS)  + #39 +','
              +#39+  TRIM(vsDADOS06NUMEROS)
          +#39+ ');';
      DM1.IBQ_Combinacoes.SQL.Clear;
      DM1.IBQ_Combinacoes.SQL.Add(vcQuery)  ;
      DM1.IBQ_Combinacoes.ExecSQL;
      if (viContarLinhas MOD 580) = 0 then
      BEGIN
         Pgbr_GERARCOMB1.Position := Trunc((viContarLinhas/(Stgr_11NumerosSorteadosM1.RowCount-1))*100);
         Pgbr_GERARCOMB1.Repaint;
         Sleep (15);
      END;
    end;
   Pgbr_GERARCOMB1.Position := 100;
   Pgbr_GERARCOMB1.Repaint;
   DM1.IBQ_Combinacoes.SQL.Clear;
   DM1.IBQ_Combinacoes.SQL.Add('COMMIT')  ;
   DM1.IBQ_Combinacoes.ExecSQL;
   DM1.IBQ_Combinacoes.Transaction.StartTransaction;
   DM1.IBQ_Combinacoes.Transaction.Commit;
   DM1.IBQ_Combinacoes.Transaction.StartTransaction;
End;


