      Stgr_09NumerosSorteados.Cells[10,viLinhas] := Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),25,2);
      Stgr_09NumerosSorteados.Cells[11,viLinhas] := Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),28,2);

      Stgr_06NumerosNaoSorteados.Cells[03,viLinhas] := ((Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),31,2)).ToInteger*-1).ToString ;
      Stgr_06NumerosNaoSorteados.Cells[04,viLinhas] := ((Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),34,2)).ToInteger*-1).ToString ;
