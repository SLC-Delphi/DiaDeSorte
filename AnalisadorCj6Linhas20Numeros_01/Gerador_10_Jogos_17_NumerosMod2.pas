Procedure T_F_GERADOR_25d17x8M2.Gravar15NumerosP1();
Var
   viContarLinhas, viContarColunas, ViContarCelulas: integer;
   vsDADOS15NUMEROS, vsDADOS10NUMEROS, vcQuery, vcNomeTabela : String;
Begin
   vcNomeTabela := ' CJTO_17POR8_15_RESULTADO';
   Pgbr_GERARCOMB1.Position := 0;
   Pgbr_GERARCOMB1.Repaint;
   for viContarLinhas := 1 to Stgr_15NumerosSorteadosP1.RowCount-1 do
   begin
      if Stgr_15NumerosSorteadosP1.Cells[19, viContarlinhas] = 'F' then
         continue;
      if Stgr_15NumerosSorteadosP1.Cells[02, viContarlinhas] = 'F'  then
         continue;
      if Stgr_15NumerosSorteadosP1.Cells[18, viContarlinhas] = 'F' then
         continue;
      vsDADOS15NUMEROS := '';
      vsDADOS10NUMEROS := '';
      // --> preparando os dados para gravar
      for viContarColunas := 01 to 15 do
      begin
         vsDADOS15NUMEROS := vsDADOS15NUMEROS + RightStr('0'+Stgr_15NumerosSorteadosP1.Cells[viContarColunas+2,viContarlinhas],2)+' ' ;
      end;
      for viContarColunas := 01 to 10 do
      begin
         vsDADOS10NUMEROS := vsDADOS10NUMEROS + RightStr('0'+((Stgr_10NumerosNaoSorteadosP1.Cells[viContarColunas,viContarlinhas].ToInteger)).ToString,2)+' ' ;
      end;
      vcQuery  := 'INSERT INTO ' +vcNomeTabela+ ' (N, DADOS, DADOS_NS) VALUES('
              +#39+  TRIM(Stgr_15NumerosSorteadosP1.Cells[1,viContarlinhas])  + #39 +','
              +#39+  TRIM(vsDADOS15NUMEROS)  + #39 +','
              +#39+  TRIM(vsDADOS10NUMEROS)
          +#39+ ');';
      DM1.IBQ_Combinacoes.SQL.Clear;
      DM1.IBQ_Combinacoes.SQL.Add(vcQuery)  ;
      DM1.IBQ_Combinacoes.ExecSQL;
      if (viContarLinhas MOD 2099) = 0 then
      BEGIN
         Pgbr_GERARCOMB1.Position := Trunc((viContarLinhas/(Stgr_15NumerosSorteadosP1.RowCount-1))*100);
         Pgbr_GERARCOMB1.Repaint;
         Sleep (20);
      END;
    end;
   DM1.IBQ_Combinacoes.SQL.Clear;
   DM1.IBQ_Combinacoes.SQL.Add('COMMIT')  ;
   DM1.IBQ_Combinacoes.ExecSQL;
   DM1.IBQ_Combinacoes.Transaction.StartTransaction;
   DM1.IBQ_Combinacoes.Transaction.Commit;
   DM1.IBQ_Combinacoes.Transaction.StartTransaction;
   Pgbr_GERARCOMB1.Position := 100;
   Pgbr_GERARCOMB1.Repaint;
End;


Procedure T_F_GERADOR_25d17x8M2.Carregar15NumerosP1();
Var
   viQuantidade, viLinhas: integer;
   vcQuery : String;
Begin
   Pgbr_REMREPET.Position := 0;
   Pgbr_REMREPET.Repaint;
   Grid15NumerosSorteados1;
   Grid10NumerosNaoSorteados1;
   sleep(200);
   Stgr_15NumerosSorteadosP1.RowCount := 50;
   Stgr_10NumerosNaoSorteadosP1.RowCount := 50;
   vcQuery :='Select count (Dados) as QTD From ( ';
   vcQuery := vcQuery + 'Select  DADOS || ' +#39+'-'+#39+ ' ||DADOS_NS as DADOS ';
   vcQuery := vcQuery + 'from CJTO_17POR8_15_RESULTADO ';
   vcQuery := vcQuery + 'group by DADOS) ';
   DM1.IBQ_Combinacoes.SQL.Clear;
   DM1.IBQ_Combinacoes.SQL.Add(vcQuery)  ;
   DM1.IBQ_Combinacoes.Prepare;
   DM1.IBQ_Combinacoes.Open;
   viQuantidade := DM1.IBQ_Combinacoes.FieldByName('QTD').AsInteger;
   vcQuery :='Select DADOS || ' +#39+'-'+#39+ ' ||DADOS_NS as DADOS ';
   vcQuery := vcQuery + 'from CJTO_17POR8_15_RESULTADO ';
   vcQuery := vcQuery + 'group by DADOS ';
   DM1.IBQ_Combinacoes.SQL.Clear;
   DM1.IBQ_Combinacoes.SQL.Add(vcQuery)  ;
   DM1.IBQ_Combinacoes.Prepare;
   DM1.IBQ_Combinacoes.Open;
   DM1.IBQ_Combinacoes.First;
   viLinhas := 1;
   while not(DM1.IBQ_Combinacoes.Eof) do
   begin
      Stgr_15NumerosSorteadosP1.Cells[00,viLinhas] := viLinhas.ToString;
      Stgr_15NumerosSorteadosP1.Cells[01,viLinhas] := '';
      Stgr_15NumerosSorteadosP1.Cells[02,viLinhas] := '';
      Stgr_10NumerosNaoSorteadosP1.Cells[00,viLinhas] := viLinhas.ToString;
      Stgr_10NumerosNaoSorteadosP1.Cells[01,viLinhas] := '';

      Stgr_15NumerosSorteadosP1.Cells[03,viLinhas] := Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),01,2);
      Stgr_15NumerosSorteadosP1.Cells[04,viLinhas] := Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),04,2);
      Stgr_15NumerosSorteadosP1.Cells[05,viLinhas] := Copy((DM1.IBQ_Combinacoes.FieldByName('DADOS').AsString),07,2);
