       // --> ordena os dados
      Falso_Linha1TodosNumeros;
      Falso_Linha2TodosNumeros;
      Falso_Linha3TodosNumeros;
      //14 e 08
      for viContarColunas := 01 to 14 do
      begin
         Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_Proc2Combinacoes14Sorteados.Cells[viContarColunas+3, viContar04])), 1] := 'V'
      end;
      for viContarColunas := 01 to 8 do
      begin
         Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_Proc2Combinacoes08NaoSorteados.Cells[viContarColunas+4, viContar04])), 1] := 'V'
      end;
      viContarBase17e08 := Stgr_Proc2Combinacoes14Sorteados.Cells[01,viContar04].toInteger;
      //cj 17 base
      for viContarColunas := 01 to 17 do
      begin
         Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_base17.Cells[viContarColunas+1, viContarBase17e08])), 2] := 'V'
      end;
      //cj 08 base
      for viContarColunas := 01 to 17 do
      begin
         Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_base17.Cells[viContarColunas+1, viContarBase17e08])), 3] := 'V'
      end;

      ViContarCelulas := 1;
      FOR viContarColunas :=1 TO 25 DO
      BEGIN
         IF (Stgr_Todos_Numeros.Cells[viContarColunas,3]='V') and (Stgr_Todos_Numeros.Cells[viContarColunas,1]='F') THEN
         BEGIN
            Stgr_Proc2Combinacoes01NaoSorteadosConv.Cells [ViContarCelulas+2, viLinhas] := RIGHTSTR ('0'+(Stgr_Todos_Numeros.Cells[viContarColunas,0]),2) ;
            ViContarCelulas := ViContarCelulas+1;
         end;
      END; // FOR viContarColunas
//      if ViContarCelulas<9 then  // 7
//         continue;
      viLinhas := viLinhas + 1;
      if viLinhas mod viQtdLinhasGravar1 = 0 then    // limpar grid
      begin
         Stgr_Proc2Combinacoes01NaoSorteadosConv.RowCount := viLinhas;
         Stgr_Proc2Combinacoes01NaoSorteadosConv.Repaint;
       //  GravarP2Comb14e08();
       //  Grid014NumerosSorteadose8NS();
       //  viLinhas := 1;

         break;


      end;
      if viContar04 mod 0900 = 0 then   // percentual
      begin
         Pgbr_GerarCombinacoes2.Position :=Trunc(viContar04/(Stgr_Proc2Combinacoes14Sorteados.RowCount-1)*100);
         Pgbr_GerarCombinacoes2.Repaint;
         Stgr_Proc2Combinacoes01NaoSorteadosConv.RowCount := viLinhas;
         Stgr_Proc2Combinacoes01NaoSorteadosConv.Repaint;
      end;
   end;
//   Pgbr_GerarCombinacoes2.Position :=100;
//   Pgbr_GerarCombinacoes2.Repaint;
   Stgr_Proc2Combinacoes14Sorteados.RowCount := viLinhas;
   Stgr_Proc2Combinacoes14Sorteados.Repaint;
   Stgr_Proc2Combinacoes08NaoSorteados.RowCount := viLinhas;
   Stgr_Proc2Combinacoes08NaoSorteados.Repaint;

   Stgr_Proc2Combinacoes01NaoSorteadosConv.RowCount := viLinhas;
   Stgr_Proc2Combinacoes01NaoSorteadosConv.Repaint;
//   GravarP2Comb14e08();
   Self.WindowState := wsMinimized;
   Self.WindowState := wsNormal;
   sleep(10) ;
End;



