            end;
         END; // FOR viContarColunas
         if ViContarCelulas<9 then  // 7
            continue;
         // conferir se os numeros sorteados e não sorteados são diferentes:
         Falso_Linha3TodosNumeros;
         for viContarColunas := 01 to 08 do
         begin
            Stgr_Todos_Numeros.Cells[(STRtoINT(Stgr_Proc2Combinacoes08NaoSorteados.Cells[viContarColunas+4, viLinhas])), 3] := 'V';
         end;
         for viContarColunas := 01 to 14 do
